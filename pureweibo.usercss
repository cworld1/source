}
.UG_left_nav li {
    margin-bottom: 7px;
}

.UG_slider_v2 li img {
    border-radius: 4px;
}
.UG_slider_v2 .pic_intro {
    border-radius: 0 0 4px 4px;
}
.W_unlogin_v4 .W_login_form .input_wrap .W_input {
    margin-left: 34px;
