@-moz-document domain("api.weibo.com") {
/* 整体 */
.chat .chatbox {
    border-radius: 4px !important;
    -webkit-transition: .3s linear;
    transition: .2s linear;
    background-color: #f2f2f2bd !important;
}

/* 侧栏 */
.chat .chatbox .msglist {
    border-radius: 4px 0 0 4px !important;
    background: #fff0 !important;
}
.user {
    background: #fff !important;
    border-radius: 4px 0 0 4px !important;
    margin: 10px 10px 0 10px;
}
.search {
    background: #fff !important;
    border-radius: 0 0 4px 4px !important;
    margin: 0 10px 10px 10px;
}
.user .left .name, .user .right .icon-more,
.search .search-wrapper.align-items_c .searchInput {
    color: #000000a3 !important;
    caret-color: #000000a3 !important;
}
.fff {
    color: #000 !important;
}
.search {
    border: none !important;
}

.chatlist {
    top: 144px !important;
    right: 0;
    left: 0;
    bottom: 0;
    margin-right: -17px;
}
