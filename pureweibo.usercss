@-moz-document domain("weibo.com"), domain("www.weibo.com") {
/* -----广告----- */
[feedtype="ad"],DIV[feedtype="ad"],
[id*="v6_pl_rightmod_ad"],#v6_pl_rightmod_noticeboard,
#v6_trustPagelet_recom_member,#v6_pl_rightmod_rank,
#v6_pl_rightmod_attfeed,#pl_content_hometip,
#v6_pl_guide_bubbletip,#v6_pl_content_hometip,
.key[node-type="recommendTopic"],.S_txt1[title="V+微博"],
.M_sc_tips, .WB_global_nav .gn_nav_list li .W_new,
.WB_feed_v3 .WB_feed_repeat .W_tips,
.WB_h5video_v2 .wbv-add-box, .WB_h5video_v2 .wbv-logo
{
   display: none !important;
}

/*隐藏姓名框的名字并自适应高度*/
.WB_feed_detail{
    background-image: none !important;
}
.WB_starcover
{
    display: none !important;
}

/*隐藏姓名框的卡片背景*/
.lev_line_v2 {
    display: none !important;
}

/*隐藏姓名框的等级和会员图标*/
.nameBox a>i.icon_member_dis,
span[node-type="levelBox"] {
    display: none !important;
}

/*隐藏播放音乐的按钮*/
div[node-type=box][i-am-music-player] {
    display: none !important;
}
.WB_red2017,
.item.S_line2:nth-of-type(1) {
    display: none !important;
}

/*自动隐藏模板设置按钮*/
.W_icon.icon_setskin {
    opacity: 0 !important;
    -moz-transition: .3s ease-in-out;
    -webkit-transition: .3s ease-in-out;
    -o-transition: .3s ease-in-out;
    background-position: -150px -275px;
    top: 3px;
    transform: scale(0.9);
    transform: translate(5px,-5px)
}

/*鼠标滑过显示模板设置按钮（已上移到右上角）*/
.W_icon.icon_setskin:hover{
    opacity: 1 !important;
    transform: scale(1.1);
    transform: translate(0,0)
} 

/*自动隐藏非橙V蓝V图标*/

.W_icon_yy
{
opacity: 0 !important;
display:none !important;
}

.W_icon_yystyle
{
opacity: 0 !important;
display:none !important;
}

.W_icon_bf.icon_bigfans
{
opacity: 0 !important;
display:none !important;
}

.W_icon_vipstyle
{
opacity: 0 !important;
display:none !important;
}

.W_icon.icon_member1
{
opacity: 0 !important;
display:none !important;
}
.W_icon.icon_member2
{
opacity: 0 !important;
display:none !important;
}
.W_icon.icon_member3
{
opacity: 0 !important;
display:none !important;
}
.W_icon.icon_member4
{
opacity: 0 !important;
