}
.W_icon.icon_member7
{
opacity: 0 !important;
display:none !important;
}
.W_icon.icon_panda
{
opacity: 0 !important;
display:none !important;
}
.W_icon.icon_club
{
opacity: 0 !important;
display:none !important;
}

.W_icon.icon_vlady
{
opacity: 0 !important;
display:none !important;
}

.W_icon.icon_wenda
{
opacity: 0 !important;
display:none !important;
}

.W_icon.icon_wenda_v2_v1
{
opacity: 0 !important;
display:none !important;
}


