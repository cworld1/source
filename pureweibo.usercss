  } 

.W_icon.icon_pf_approve_gold
  {
    opacity: 1 !important;
  } 

.W_icon.icon_verify_v
  {
    opacity: 1 !important;
  } 


 /*鼠标滑过显示非橙V蓝V图标*/
.W_icon:hover
  {
    opacity: 1 !important;
    -moz-transition: opacity .25s ease-in-out;
    -webkit-transition: opacity .25s ease-in-out;
    -o-transition: opacity .25s ease-in-out;
  } 

.W_icon_yy:hover
  {
    opacity: 1 !important;
    -moz-transition: opacity .25s ease-in-out;
    -webkit-transition: opacity .25s ease-in-out;
    -o-transition: opacity .25s ease-in-out;
  } 

    /*自动隐藏底部私信聊天*/
.webim_fold
  {
    opacity: 0 !important;
  } 
 /*鼠标滑过显示底部私信聊天*/
.webim_fold:hover
  {
    opacity: 1 !important;
    -moz-transition: opacity .25s ease-in-out;
    -webkit-transition: opacity .25s ease-in-out;
    -o-transition: opacity .25s ease-in-out;
  } 


/* -----细节美化----- */

/* 动画 */
.layer_menu_list ul li a,
.gn_topmenulist_search .gn_topmenulist ul li a,
.WB_global_nav .S_ficon_dis,
.WB_global_nav a.S_ficon_dis:hover,
.WB_global_nav a:hover .S_ficon_dis {
    transition: .3s;
}
/* 背景 */
body, .WB_miniblog {
    background: url(//tvax1.sinaimg.cn/large/0060ksKkly1gbs5b1as5ej31z4140wj0.jpg) !important;
    background-repeat: repeat;
    background-attachment: scroll;
    background-position: left top;
}
.WB_frame {
    background: #f7f7f799 !important;
    margin: 10px auto;
    border-radius: 4px;
    padding: 12px 2px 10px 20px;
}

/* 卡片 */
.WB_cardwrap:not(#home_new_feed_tip), .S_bg2, blockquote,
.PCD_connectlist .member_box .member_li, .m-main-nav,
.card-wrap, .card
{
    transition: .3s;
    background: #fff;
    box-shadow: none;
    border-radius: 4px;
    border: none;
}
.WB_tab_a, .W_person_info .cover {
    transition: .3s;
    background: #fff;
    box-shadow: none;
    border-radius: 4px 4px 0 0;
}
div#home_new_feed_tip {
    transition: .3s;
    background: #fff;
    box-shadow: none;
    border-radius: 0 0 4px 4px;
}
.S_line2, .WB_feed_handle .WB_row_line {
    border: none !important;
}
.S_line1, .W_btn_prev, .W_btn_next, .W_btn_b {
    border-color: #0000000d;
}
.S_bg1 {
    background-color: #f8f8f8;
}
.SW_fun_bg:hover, .SW_fun_bg_active {
    background-color: #fff;
}
.WB_tab_b {
    box-shadow: none;
    border-radius: 4px;
}
.PCD_connectlist.PCD_connectlist_spe {
    background-color: #00000008;
}
.card-review {
    margin-top: 0;
    border-top: none;
}
.m-review .list {
	transition: .3s;
    background: #fff;
    box-shadow: none;
    border-radius: 4px;
    border: none;
	padding: 5px 15px;
	margin: 5px 0;
}

/* 输入 */
.send_weibo .input, .send_weibo .input_simple {
    border: none;
    background-color: #0000000a;
    border-radius: 4px;
    padding: 5px 10px;
    box-shadow: none;
}
.con.W_input {
    background-color: #0000000a;
}
.layer_setkeyword .con .W_input {
    border: none;
    background-color: #fff0;
}

/* 搜索框 */
.S_line3,.gn_header.clearfix .W_input, .m-search .searchbox input, .card-sender .func .input-wrap .input textarea {
    box-shadow: none;
    border: none;
    border-radius: 4px;
    background-color: #0000000a;
}
.WB_global_nav .gn_search_v2 {
    box-shadow: none;
    border: none;
    border-radius: 4px;
    background-color: #0000000a;
    margin: 7px 0 0 7px;
    padding: 3px 0;
}
.WB_global_nav .gn_search_v2 .placeholder, .WB_global_nav .gn_search_v2 .W_input {
    padding: 7px 31px 7px 12px;
    height: 20px;
    line-height: 20px;
    box-shadow: none;
    border: none;
}

/* 弹出框 */
.layer_menu_list, .WB_global_nav .gn_topmenulist, .W_layer_pop .content {
    background: #fff;
    border: none;
    border-radius: 4px;
    padding: 5px;
    box-shadow: 0px 2px 8px #0003;
}
.layer_menu_list ul li a {
	transition: .3s;
}
.layer_menu_list ul li.cur a, .layer_menu_list ul li a:hover {
    border-radius: 4px;
    background: #00000008;
}

/* 按钮 */
.W_btn_a, .btn_34px, .s-btn-b, .s-btn-a {
    transition: .3s;
    background: #ff8140;
    border-radius: 4px;
    padding: 1px 11px;
    border: none;
    color: #fff;
    box-shadow: 0 0 #0000;
}
.W_btn_a:hover, .btn_34px:hover, .s-btn-b:hover,
.s-btn-a:hover {
    background: #fff;
    border-radius: 4px;
    padding: 1px 11px;
    border: none;
    color: #ff8140;
    box-shadow: 0 2px 5px #00000026;
}
.W_btn_a_disable, .W_btn_a_disable:hover {
    padding: 1px 11px;
    background: #ffc09f;
    color: #fff;
    border: none;
    box-shadow: none;
}
.W_btn_c .W_ficon, .W_btn_d .W_ficon {
    transition: .3s;
}
.W_btn_c:hover .W_ficon, .W_btn_d:hover .W_ficon {
    color: #ff8140 !important;
}

.W_btn_b, .W_input, .SW_fun_bg {
    transition: .3s;
    background: #00000014;
    box-shadow: none;
    border-radius: 4px;
    border: none;
}
.W_btn_b:hover {
    background: #fff;
    border-radius: 4px;
    border-color: #fff0 !important;
    box-shadow: 0 2px 5px #00000026;
}
.W_layer .S_line1, .W_layer .W_btn_prev, .W_layer .W_btn_next, .W_layer .W_btn_b {
    border-color: #ff8140;
    color: #ff8140;
}
.WB_card_vote .vote_con1 .item:hover {
    border-color: #ffab8029;
    background: #ffab8029;
}

/* 窗口 */
.W_layer .content {
    position: relative;
    background: #fff;
    border-radius: 4px;
    border-top: none;
}

/* 顶栏 */
.WB_global_nav {
    border: none;
    box-shadow: none;
    background: #fff;
}
.WB_global_nav .ficon_send {
    color: #696e78 !important;
    border-radius: 2px;
    background: #fff0 !important;
}
.WB_global_nav .ficon_send:hover {
    color: #ff8140 !important;
}

.W_fixed_top {
	top: 55px !important;
}
.WB_tab_a .tab_box_c, .WB_tab_a .tab_box_c .tab .curr a,
.card-together .card-more-a a {
    border: none;
	box-shadow: none ;
}
.W_fixed_top .WB_tab_a {
	transition: .3s;
	box-shadow: 0 2px 10px #00000021 !important;
	margin: 0 10px;
    border-radius: 4px;
}
.WB_tab_a .tab_box_c .tab_li .ani_border {
    border-radius: 4px 4px 0 0;
    left: 0;
    right: 0;
    height: 3px;
	margin: 0 35%;
}
.WB_global_nav_alpha {
    box-shadow: 0 2px 10px #00000021 !important;
}

/* 侧栏 */
.WB_left_nav {
    position: relative;
    right: 10px;
    width: 150px;
}
.WB_left_nav .S_txt1, .WB_left_nav a.S_txt1:hover, .WB_left_nav .S_ficon, .WB_left_nav a:hover .S_ficon, .WB_left_nav a.S_ficon:hover {
    color: #595959;
}
.WB_left_nav .lev_Box.lev_Box_noborder:nth-child(1),
.WB_left_nav .lev_Box.lev_Box_noborder:nth-child(2),
.WB_left_nav .lev_Box.lev_Box_noborder:nth-child(3) {
    background-color: #ffffffb0;
}
.WB_left_nav .lev_Box.lev_Box_noborder:nth-child(1) {
    border-radius: 4px 4px 0 0;
    padding-top: 2.5px;
}
.WB_left_nav .lev_Box.lev_Box_noborder:nth-child(3) {
    border-radius: 0 0 4px 4px;
    padding-bottom: 2.5px;
}
.WB_left_nav .lev_Box.lev_Box_noborder:nth-child(1) a,
.WB_left_nav .lev_Box.lev_Box_noborder:nth-child(2) a,
.WB_left_nav .lev_Box.lev_Box_noborder:nth-child(3) a {
    margin: 2.5px 5px;
}
.WB_left_nav .lev a {
    transition: .3s;
    padding: 0 0 0 15px;
    height: 30px;
    line-height: 30px;
    border-radius: 4px;
    margin: 5px 0;
}
.WB_left_nav .lev_Box h3 {
    height: 40px;
    line-height: 40px;
}
.WB_left_nav .lev a:hover, .WB_left_nav .lev_curr:hover {
    background-color: #ff81401f;
}
.WB_left_nav .lev_curr, .WB_left_nav .levmore .more {
    background-color: #ff814030;
    color: #ff8140;
}

.PCD_left_nav .WB_innerwrap {
    padding: 10px;
    position: relative;
    z-index: 1;
}
.PCD_left_nav .lev .lev_a {
    transition: .3s;
    padding: 5px 14px;
    height: 30px;
    line-height: 30px;
    border-radius: 4px;
    margin: 2.5px 0;
}
.PCD_left_nav .lev .lev_a:hover {
    background-color: #ff81401f;
}
.PCD_left_nav .lev .current {
    background-color: #ff814030;
    color: #ff8140;
}

/* -----修复与优化----- */

/* 边线 */
.card-top, .card-head, .card-act, .WB_feed_repeat .repeat_list .list_li, .WB_feed_repeat .WB_repeat, .WB_cardmore, .W_layer .W_layer_title, .card-together {
    border: none;
}

/* 提示 */
.W_tips.tips_warn {
    border: none;
    background: #fff;
    border-radius: 4px;
}

/* 回顶 */
.W_gotop {
    transition: .3s;
    background: #fff;
    width: 40px;
    height: 35px;
    border-radius: 4px;
    padding-top: 5px;
    opacity: 0.6;
    overflow: hidden;
}
#base_scrollToTop::after {
    transition: .3s;
    opacity: 0;
    content: '\A回到\A顶部';
    width: 7px;
    height: 7px;
    white-space:pre;
    color: #0000009e;
    top: 0;
}
#base_reload::after {
    transition: .3s;
    opacity: 0;
    content: '\A点击\A刷新';
    width: 7px;
    height: 7px;
    white-space:pre;
    color: #0000009e;
    top: 0;
}
#base_scrollToTop:hover::after,#base_reload:hover::after {
    position: relative;
    opacity: 1;
    color: #0000009e;
    top: -30px;
}
.W_gotop em {
    transition: .3s;
    color: #0000009e;
}
.W_gotop:hover em {
    opacity: 0;
    color: #0000009e;
    transform: translatey(-15px);
}

/* 其他 */

.WB_global_nav .W_arrow_bor_t i, .WB_global_nav .W_arrow_bor_t em {
    display: none;
}

.WB_global_nav .gn_topmenulist_search .gn_topmenulist>div>.selectbox>.selectbox {
    order: 1;
}
.WB_global_nav .gn_topmenulist>div>.selectbox>li {
    order: 2;
}

.W_layer .S_bg1, .W_layer blockquote, .W_layer .SW_fun_bg:hover, .W_layer .SW_fun_bg_active, .WB_tab_b .opt_bar {
    background-color: #fff0;
}

.PCD_tab .tb_tab .tab_link .ani_border {
	height: 3px;
	width: 50%;
    border-radius: 4px 4px 0 0;
    margin: 0 25%;
    background: #f7691dd1;
    -webkit-transition: all .2s linear;
    transition: all .2s linear;
    overflow: hidden;
}

.PCD_header .pf_wrap, .cover_wrap.banner_transition {
    border-radius: 4px 4px 0 0;
}
.card-together {
    background: #00000005;
}
.layer_personcard .nc_head {
	width: auto;
	margin: 10px;
    border-radius: 4px;
}
.layer_personcard .c_btnbox .W_btn_b {
    border: 1px solid #0000000f;
}
.WB_feed_v3 .WB_media_a li, html .WB_feed_v3 .WB_media_a_m1 .WB_video_h5_v2, .Tf_media_a_mn li, .media-piclist ul li img,
.media-video-a .thumbnail {
	overflow: hidden;
    border-radius: 4px;
}

.WB_media_view .pic_choose_box li a {
    transition: opacity .3s;
}
.WB_media_view .pic_choose_box li a:hover, .WB_media_view .pic_choose_box li a.current, .WB_media_view .pic_choose_box li img {
    border-radius: 4px;
	background-color: #fa7d3c;
}

.WB_func .WB_handle li.hover {
	transition: .3s;
    visibility: visible;
	opacity: 0;
}
.WB_feed_v3 .WB_feed_repeat .repeat_list .WB_func:hover .WB_handle li.hover {
	opacity: .6;
}
.WB_feed_v3 .WB_feed_repeat .repeat_list .WB_func:hover .WB_handle li.hover:hover {
	opacity: 1;
}
.WB_feed_repeat .WB_face img {
	border-radius: 50%;
}
.S_txt2_bg {
	transition: .3s;
    background-color: #00000014;
}
.S_txt2_bg:hover {
    background-color: #808080;
}

/*=== 游客模式 ===*/
.UG_left_nav ul{
    width: 130px;
	position: relative;
	left: -5px;
	top: 7px;
}
.UG_left_nav .nav_item {
